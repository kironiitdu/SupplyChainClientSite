                                }

                            }
                        }
                    }
                });
                var d = new Date();
                var dateTime = d.toJSON();
                jQuery("#grid").kendoGrid({
                    excel: {
                        fileName: "Stock_Inventory_" + dateTime + ".xlsx"
                    },
                    dataSource: dataSource,
                    filterable: true,
