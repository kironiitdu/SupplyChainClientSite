        // Fucntion Duplicate check
        function duplicate(listOfDuplicate, imei) {
            if (!listOfDuplicate) {
                return false;
            } else {
                for (var i = 0; i < listOfDuplicate.length; i++) {
                    if (listOfDuplicate[i].imei == imei || listOfDuplicate[i].imei_no2 == imei) {
                        return true;
                    }
                }
                return false;
            }
        }

