                    function colorDropDownEditorGift(container, options) {
                        jQuery('<input id="colorIdPromotion" required data-text-field="color_name" data-value-field="color_id" data-bind="value:' + options.field + '"/>')
                            .appendTo(container)
                            .kendoComboBox({
                                autoBind: true,
                                placeholder: "-Select-",
