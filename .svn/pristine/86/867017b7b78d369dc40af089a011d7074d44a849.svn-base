                                change: function (e) {
                                    var dataItem = this.dataItem(e.item);
                                    var cId = dataItem.color_id;

                                    console.log(dataItem.product_name);
                                    var grid = $("#PromotionList").data("kendoGrid");
                                    var sel = grid.select();
                                    var sel_idx = sel.index();
