    // Plus Function

    function quantityPlusFunction(productId) {
        var grid = $("#RequisitionDetailsGrid").data("kendoGrid")._data;
        for (var i=0; i<grid.length;i++) {
            var firstItem = $('#RequisitionDetailsGrid').data().kendoGrid.dataSource.data()[i];
            var line_total = 0;
            if (grid[i].product_id == productId) {
                firstItem.set('quantity', grid[i].quantity + 1);
                line_total= parseFloat(grid[i].quantity)*parseFloat(grid[i].mrp_price);
             
            }

        }

    }

    function quantityMinusFunction(productId) {
        var grid = $("#RequisitionDetailsGrid").data("kendoGrid")._data;
       
        for (var i=0; i<grid.length;i++) {
            var firstItem = $('#RequisitionDetailsGrid').data().kendoGrid.dataSource.data()[i];
            if (grid[i].product_id == productId) {
                if (grid[i].quantity == 1) {
                    alert("Invalid Quantity");
                    return false;
                }
                firstItem.set('quantity', grid[i].quantity - 1);
            }

        }

    }
    //@ViewBag.t1 = "sdsd";




    //Start

    var t;

    function deleteFunction(productId) {
        var grid = $("#RequisitionDetailsGrid").data("kendoGrid")._data;
        for (var i=0; i<grid.length;i++) {
            if (grid[i].product_id == productId) {
                var dataRow = $('#RequisitionDetailsGrid').data("kendoGrid").dataSource.get(productId);

                console.log(dataRow);
                $('#RequisitionDetailsGrid').data("kendoGrid").dataSource.remove(dataRow);
                $('#RequisitionDetailsGrid').data('kendoGrid').refresh();



            }

        }


    }





</script>

