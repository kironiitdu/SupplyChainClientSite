            var cost = costValue();
            var packCost = costValueWithPackage();
            $("#cost_price").val(cost);
            $("#total_package_cost").val(packCost);

            $("#amra_margin_label").text(($("#total_package_cost").val() * ($("#amra_margin").val()/100)).toFixed(2));
